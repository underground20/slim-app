{
    "require": {
        "slim/slim": "^4.7",
        "laminas/laminas-diactoros": "^2.5",
        "php-di/php-di": "^6.3",
        "symfony/console": "^5.2",
        "doctrine/orm": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "doctrine/data-fixtures": "^1.4",
        "fzaninotto/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "app": "php bin/app.php --ansi",
        "test": "vendor/bin/phpunit --no-coverage"
    }
}
